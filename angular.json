{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "overlay-spinner": {
      "projectType": "library",
      "root": "projects/overlay-spinner",
      "sourceRoot": "projects/overlay-spinner/src",
      "prefix": "asm",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/overlay-spinner/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/overlay-spinner/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/overlay-spinner/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/overlay-spinner/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "showcase": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:resolver": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/showcase",
      "sourceRoot": "projects/showcase/src",
      "prefix": "asm",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/showcase",
            "index": "projects/showcase/src/index.html",
            "browser": "projects/showcase/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/showcase/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/showcase/public"
              }
            ],
            "styles": [
              "@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/showcase/src/styles.scss",
              "dist/overlay-spinner/styles/style.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kB",
                  "maximumError": "4kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "showcase:build:production"
            },
            "development": {
              "buildTarget": "showcase:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/showcase/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/showcase/public"
              }
            ],
            "styles": [
              "@angular/material/prebuilt-themes/cyan-orange.css",
              "projects/showcase/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "form-control-error-detail": {
      "projectType": "library",
      "root": "projects/form-control-error-detail",
      "sourceRoot": "projects/form-control-error-detail/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/form-control-error-detail/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/form-control-error-detail/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/form-control-error-detail/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/form-control-error-detail/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "commons": {
      "projectType": "library",
      "root": "projects/commons",
      "sourceRoot": "projects/commons/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/commons/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/commons/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/commons/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/commons/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "material-file-uploader": {
      "projectType": "library",
      "root": "projects/material-file-uploader",
      "sourceRoot": "projects/material-file-uploader/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/material-file-uploader/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/material-file-uploader/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/material-file-uploader/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/material-file-uploader/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  }
}
